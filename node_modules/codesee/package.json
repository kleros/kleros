{
  "name": "codesee",
  "version": "0.364.0",
  "description": "CodeSee instrumentation and analysis to support developers with continuous code understanding",
  "bin": {
    "codesee": "./bin/cli.js"
  },
  "main": "bin/cli.js",
  "scripts": {
    "format": "prettier --write '{bin,src}/**/*.{ts,js}'",
    "lint": "eslint '{bin,src}/**/*.{ts,js}' --fix",
    "pre-commit": "lint-staged",
    "cliinit": "tsc && node bin/cli.js init",
    "map": "tsc && node bin/cli.js map",
    "detect-languages": "tsc && node bin/cli.js detect-languages",
    "metadata": "tsc && node bin/cli.js metadata",
    "insight": "tsc && node bin/cli.js insight",
    "upload": "tsc && node bin/cli.js upload",
    "test": "jest",
    "watch": "jest --watchAll",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsc",
    "prepare": "yarn build",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "CodeSee",
    "continuous",
    "understanding"
  ],
  "author": "CodeSee",
  "license": "UNLICENSED",
  "types": "./dist/global.d.ts",
  "files": [
    "dist/global.d.ts",
    "dist",
    "bin/cli.js"
  ],
  "dependencies": {
    "@codesee/domain-core": "^0.364.0",
    "@codesee/shared-constants": "^0.364.0",
    "@supercharge/promise-pool": "^1.7.0",
    "@yarnpkg/lockfile": "1.1.0",
    "axios": "^0.21.1",
    "axios-debug-log": "^0.1.0",
    "chalk": "^4.1.1",
    "commander": "^9.2.0",
    "filing-cabinet": "3.1.0",
    "handlebars": "^4.7.7",
    "micromatch": "^4.0.4",
    "object-path": "^0.11.5",
    "precinct": "8.1.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@codesee/prettier-config": "^0.31.1",
    "@types/filing-cabinet": "^2.5.1",
    "@types/madge": "^5.0.0",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^14.0.26",
    "@types/object-path": "^0.11.1",
    "@types/semver": "^7.3.8",
    "@types/yarnpkg__lockfile": "1.1.5",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "prettier": "@codesee/prettier-config",
  "lint-staged": {
    "{bin,src}/**/*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
